#!bin/bash
#skript2
#algus
who | cut -f1 -d" " | cut -f2 -d"." > perenimi # l천ikab perenime ja salvestab
who | cut -f1 -d" " | cut -f1 -d"." > eesnimi # l천ikab eesnime ja salvestab
paste -d"."  perenimi eesnimi > nimed #kokku paneb
cat nime | sort | uniq # sorteerib v채ljundi ja korduvad eemaldab
